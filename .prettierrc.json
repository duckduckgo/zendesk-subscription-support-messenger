{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "trailingComma": "all",
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": true,
  "useTabs": false,
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "singleAttributePerLine": false,
  "endOfLine": "lf"
}
